/**
 * 4 bit multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux4 {
    IN a[4], b[4], sel;
    OUT out[4];

    PARTS:
    Not(in=sel,out=notsel);
    
    Mux2(a[0..1]=a[0..1],b[0..1]=b[0..1],sel=sel,out[0..1]=out[0..1]);
    Mux2(a[0..1]=a[2..3],b[0..1]=b[2..3],sel=sel,out[0..1]=out[2..3]);
}

